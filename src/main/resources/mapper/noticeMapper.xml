<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.myapp.notice.mapper.NoticeMapper">

    <select id="findAll" resultType="com.project.myapp.dto.Notice">
        SELECT *
        FROM notice
                 limit 5000
    </select>

    <select id="findByPage" parameterType="Integer" resultType="com.project.myapp.dto.Notice">
        SELECT *
        FROM notice
                 LIMIT #{startIdx},10
    </select>
</mapper>